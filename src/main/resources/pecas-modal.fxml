<?xml version="1.0" encoding="UTF-8"?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projeto2.Desktop.PecasModalController">
   <children>
      <HBox alignment="TOP_RIGHT" prefHeight="30.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" text="X" onAction="#handleClose" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   <children>
      <Label text="Select Pecas" />
      <ListView fx:id="pecasListView" prefHeight="200.0" prefWidth="200.0" />
      <Button mnemonicParsing="false" text="Select" onAction="#handleSelect"/>
   </children>
</VBox>